{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@wonderlandlabs/forest-io",
  "author": "David Edelhart",
  "module": "dist/forest-io.esm.js",
  "size-limit": [
    {
      "path": "dist/forest-io.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/forest-io.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.0",
    "husky": "^7.0.4",
    "lodash.get": "latest",
    "size-limit": "4.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@wonderlandlabs/forest": "latest",
    "path.join": "^1.0.0"
  }
}
